{
  "override_if_exists": false,
  "index": {
    "name": "open-library",
    "storage_type": "tempdir",
    "fields": {
      "title": {
        "type": "text",
        "stored": true,
        "indexed": true
      },
      "authors": {
        "type": "text",
        "stored": true,
        "indexed": true,
        "multi": true
      },
      "rating": {
        "type": "f64",
        "stored": true,
        "indexed": true
      },
      "edition_key": {
        "type": "string",
        "stored": true,
        "indexed": false
      }
    },
    "search_fields": ["title", "authors"],
    "max_concurrency": 2,
    "set_conjunction_by_default": true,
    "use_fast_fuzzy": true
  }
}
